
      <h1>Delete Listing</h1>
      <div class="form">
        {{#with cub}}
        <form action="/delete/{{id}}" method="POST">
          <label for="name">Name</label>
          <input type="text" id="name" name="name" value="{{name}}" disabled>
          <label for="name">Description</label>
          <textarea id="name" name="description" disabled>{{description}}</textarea>
          <label for="image">ImageUrl</label>
          <input type="text" id="image" name="imageUrl" value="{{imageUrl}}" disabled>
          <label for="difficulty">Difficulty</label>
          <input id="difficulty" name="difficultyLevel" value="{{difficulty}}" disabled>
          <input type="submit" value="Confirm Delete">
        </form>
        {{/with}}
      </div>
    